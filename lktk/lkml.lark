lkml: pair (NEWLINE pair)*
pair: CNAME ":" value
dict: "{" [pair ("," pair)*] "}"
arr: "[" [value ("," value)*] "]"
?value: "yes" -> yes
      | "no" -> no
      | arr
      | dict
      | ESCAPED_STRING

%import common.CNAME
%import common.ESCAPED_STRING
%import common.NEWLINE
%import common.WS
%ignore WS
