lkml: pair (NEWLINE pair)*
pair: CNAME ":" expr
dict: "{" [pair ("," pair)*] "}"
arr: "[" [expr ("," expr)*] "]"
?expr: "yes" -> yes
     | "no" -> no
     | arr
     | dict
     | ESCAPED_STRING

%import common.CNAME
%import common.ESCAPED_STRING
%import common.NEWLINE
%import common.WS
%ignore WS
